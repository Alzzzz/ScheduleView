<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CalendarMarkView">
        <!-- 行数 不包括表头  unclude row header-->
        <attr name="cmv_rowsnum" format="integer"/>
        <!-- 列数 不包括表头 unclude colum header-->
        <attr name="cmv_columnum" format="integer"/>
        <!-- 行头的高度 -->
        <attr name="cmv_rowheader_height" format="dimension"/>
        <!-- 列头的宽度 -->
        <attr name="cmv_columheader_width" format="dimension"/>

    </declare-styleable>
</resources>